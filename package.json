{
  "name": "@ornament/react-native-chatbot",
  "version": "0.0.1",
  "description": "React Native Chatbot TS",
  "private": true,
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "start-storybook -p 9001 -c ./.storybook --ci",
    "story": "build-storybook -o ./docs",
    "build": "tsc",
    "defs": "tsc --declaration --outDir . --emitDeclarationOnly",
    "lint-es": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "lint-ts": "./node_modules/.bin/tsc --noEmit --skipLibCheck",
    "lint": "npm run lint-es && npm run lint-ts"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ornament-Health/react-native-chatbot"
  },
  "keywords": [
    "react",
    "chat",
    "chatbot"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ornament-Health/react-native-chatbot/issues"
  },
  "homepage": "https://github.com/Ornament-Health/react-native-chatbot#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/core": "^5.3.21",
    "@storybook/react": "^5.3.21",
    "@types/react": "^16.9.56",
    "@types/react-native": "^0.63.35",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-native-web": "^0.14.8",
    "core-js": "^3.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-universe": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "react-native": "^0.63.3",
    "react-native-svg": "^12.1.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-web": "^0.14.8",
    "ts-loader": "^8.0.11",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">= 0.14.0 < 17.0.0-0",
    "react-native": ">= 0.63.3",
    "react-native-svg": ">= 12.1.0",
    "tslib": ">= 2.0.0"
  },
  "engines": {
    "npm": ">=6.9.0",
    "node": ">=10.0.0"
  }
}
